package conditionaloperators;

public class example8 {

	public static void main(String[] args) {
		int number=-23;
		if(number<=0) 
		{
			System.out.println("positive number");
			
		}
		else if(number<=0) 
		{
			System.out.println("negative number");
			
		}
		else 
		{
			System.out.println("invalid number");
		}

	}

}
