package OopsClassandObjects;

public class Oops1 {
	public int add(int n1,int n2) 
	{
		int r=n1+n2;
		return r;	
	}
	
	
	
		public static void main(String[] args) 
		{
			int num1=10;
			int num2=20;
			Oops1 oops=new Oops1();
			int result=oops.add(num1, num2);
			System.out.println(result);
		}
}
