package conditionaloperators;

public class Nestedforloop4 {

	

	public static void main(String[] args) {
		int i,j,r;
		
		
		for(i=0;i<=10;i++) 
		{
			for(j=1;j<=i;j++) 
			System.out.print(" ");
			for(j=1;j<=2*i-1;j++)
				System.out.print("*");
			System.out.print("\n");	
		}
		for(i=10-1;i>=1;i++);
	}

}
