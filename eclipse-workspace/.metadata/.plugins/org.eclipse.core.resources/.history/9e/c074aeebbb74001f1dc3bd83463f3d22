package ClassAndObject;

public class ClassndObj {
	String name="";
	String proc="";
	int ram=0;
	int price=0;
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ClassndObj lap1=new ClassndObj();
		lap1.name="dell";
		lap1.proc="i3";
		lap1.ram=8;
		lap1.price=4000;
		System.out.println(lap1.ram);

	}

}
