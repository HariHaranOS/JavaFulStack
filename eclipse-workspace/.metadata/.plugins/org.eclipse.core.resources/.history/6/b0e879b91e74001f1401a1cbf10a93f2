package Corejava;

public class MethodUsingStatic {
	public int subNumbers(int x,int y) {
		int subtract=x-y;
		return subtract;
	}

	public static void main(String[] args) {
		int num1=50,num2=30;
		MethodUsingStatic obj=new MethodUsingStatic ();
		int outcome=obj.subNumbers(num1, num2);
		System.out.println("The results is: "+outcome);

	}

}
