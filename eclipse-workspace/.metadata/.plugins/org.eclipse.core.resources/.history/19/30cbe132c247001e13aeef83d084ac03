package conditionaloperators;

public class example7 {

	public static void main(String[] args) {
		int a=24;
		int b=45;
		int c=67;
		
		if(a>=b)
		{
			System.out.println("The lowest value:");
			
		}
		else if(b<=c) 
		{
			System.out.println("The greatest value:");
		}
		else 
		{
			System.out.println("invalid results");
		}

	}

}
