package example1;

public class Example {

	public static void main(String[] args) {
		int num1=10,num2=20;
		System.out.println("num1="+num1);
		System.out.println("num2="+num2);
		num1+=num2;
		System.out.println("The value for addition is "+num1);
		System.out.println('\n');
		
		int num3=140,num4=40;
		System.out.println("num3="+num3);
		System.out.println("num4="+num4);
		num3-=num4;
		System.out.println("The value for subraction is "+num3);
		System.out.println('\n');
		
		int num5=30,num6=3;
		System.out.println("num5="+num5);
		System.out.println("num6="+num6);
		num5*=num6;
		System.out.println("The value for multiple is "+num5);
		System.out.println('\n');
		
		int num7=60,num8=6;
		System.out.println("num7="+num7);
		System.out.println("num8="+num8);
		num7/=num8;
		System.out.println("The value division is "+ num7);
		
		
		
		
		
  }

}
