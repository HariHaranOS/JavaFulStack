package Inheritaceexample;

public class Animal {
	public void eat()
	{
		System.out.println("Animal is eating");
		
	}
	class dog extends Animal
	{
		void bark() 
		{
			System.out.println("Dog is barking");
		}
	}
	public class main{
	public static void main(String[] args) {
		Animal mydog=new Animal();
		mydog.eat();
		((dog) mydog).bark();
	}	

	}
	
}
