package exceptionEx;

public class exception {

	public static void main(String[] args) {
		int i=0;
		int j=0;
		try 
		{
			j=8/i;
			
		}
		catch(Exception e) 
		{
			System.out.println("cannot divisible by zero");
		}
		System.out.println(j);
		System.out.println("Handing the exception");
	}

}
