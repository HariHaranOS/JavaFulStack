package example1;

public class Example {

	public static void main(String[] args) {
		int num1=10,num2=20;
		System.out.println("num1="+num1);
		System.out.println("num2="+num2);
		num1+=num2;
		System.out.println("num1="+num1);
		System.out.println('\n');
		
		int num3=10,num4=20;
		System.out.println("num3="+num3);
		System.out.println("num4="+num4);
		num3-=num4;
		
		
		System.out.println("num3-="+num3);
		
		
		
  }

}
