package Oopsexample1;
class appform
{
	String name;
	int rollno;
	String mobilenumber;
	
	public void display() 
	{
		System.out.println(name);
		System.out.println(rollno);
		System.out.println(mobilenumber);
		
	}
	public static void main(String[]args) 
	{
		appform codeio=new appform();
		codeio.mobilenumber="98615142383";
		codeio.name="hari";
		codeio.rollno=789;
		codeio.display();
		
	}
	
}


	

