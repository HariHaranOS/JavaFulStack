class Course
{
	private static final String name = null;
	private static final String age = null;
	public static final String Coursename = null;
	String courseName;
	int courseId;
	
	public Course() 
	{
		this.courseName=courseName;
		this.courseId=courseId;
	}
	public String getCoursename() 
	{
		return courseName;
	}
	public int courseId() 
	{
		return courseId;
	}

public class oops1{
	public static void main(String[]args) 
	{
		
	Oops1 oops=new Oops1();
	oops.name="hari";
	oops.age=20;
	
}
}
}